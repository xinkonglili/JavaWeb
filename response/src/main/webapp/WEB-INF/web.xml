<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0"
         metadata-complete="true">
    

    <servlet>
        <servlet-name>file</servlet-name>
        <servlet-class>com.jinli.servlet.FileServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>file</servlet-name>
        <url-pattern>/down</url-pattern>
    </servlet-mapping>



    <servlet>
        <display-name>UploadServlet</display-name>
        <servlet-name>UploadServlet</servlet-name>
        <servlet-class>com.jinli.servlet.UploadServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>UploadServlet</servlet-name>
        <url-pattern>/TomcatTest/UploadServlet</url-pattern>
    </servlet-mapping>


    <servlet>
        <display-name>TestJspHttp</display-name>
        <servlet-name>TestJspHttp</servlet-name>
        <servlet-class>com.jinli.servlet.TestJspHttp</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>TestJspHttp</servlet-name>
        <url-pattern>/TestJspHttp</url-pattern>
    </servlet-mapping>


    <servlet>
        <display-name>TestJspDatabase</display-name>
        <servlet-name>TestJspDatabase</servlet-name>
        <servlet-class>com.jinli.servlet.TestJspDatabase</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>TestJspDatabase</servlet-name>
        <url-pattern>/TestJspDatabase</url-pattern>
    </servlet-mapping>

    <servlet>
        <display-name>ImageServlet</display-name>
        <servlet-name>ImageServlet</servlet-name>
        <servlet-class>com.jinli.servlet.ImageServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ImageServlet</servlet-name>
        <url-pattern>/ImageServlet</url-pattern>
    </servlet-mapping>



</web-app>